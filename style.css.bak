h1 { 
  background-color:blue;
  color: black;
}
body {
    color: green;
    font-size: 14px;
}
.planet-table{
	border-collapse: collapse;
	width: 25em;
	background-color:grey;
	border-radius:5px;
	margin:25px auto;
}

.planet-table th tr{
	border-bottom: 1px dashed black;
}

.planet-table th tr td{
	letter-spacing: 0px;
	color:grey;
	padding:8px;
}