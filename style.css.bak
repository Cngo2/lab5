body {
	background-color: #B0CFDE;
    color: #2C3539;
    font-size: 14px;
	margin: 0px
}
table, th, td {
  border: 3px solid black;
}
.planet-table{
	text-align: center;
	white-space: nowrap;
	width: 100%;
	border-collapse: collapse;
	background-color:#808080;
	border-radius:10px;
	margin:25px auto;
}

.planet-table th{
	color: black;
}

.planet-table th tr td{
	text-align: center;
	letter-spacing: 0px;
	color:#2C3539;
	padding:8px;
}

.column {
  display: block;
  margin-left: auto;
  margin-right: auto;
  width: 50%;
}

.planet-table tr:nth-child(even) {
  background-color: #D6EEEE;
}

.pic {
